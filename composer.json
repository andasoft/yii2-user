{
    "name": "andatech/yii2-user",
    "description": "Yii 2 for User",
    "type": "yii2-extension",
    "keywords": ["yii2","extension"],
    "license": "GPL-3.0",
    "minimum-stability": "stable",
    "support": {
        "source": "https://github.com/andatech/yii2-user.git",
        "issues": "https://github.com/andatech/yii2-user/issues"
    },
    "authors": [
        {
            "name": "Anda Tech",
            "email": "kuakling@gmail.com",
            "role" : "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "andatech/yii2-core":"*",
        "dmstr/yii2-adminlte-asset": "2.*",
        "mdmsoft/yii2-admin": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "anda\\user\\": ""
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
